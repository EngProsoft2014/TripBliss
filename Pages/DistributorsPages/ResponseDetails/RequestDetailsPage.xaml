<?xml version="1.0" encoding="utf-8" ?>
<Controls:CustomControl xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TripBliss.Pages.DistributorsPages.ResponseDetailes.RequestDetailsPage"
             xmlns:tabView="clr-namespace:Syncfusion.Maui.TabView;assembly=Syncfusion.Maui.TabView"
             xmlns:syncfusion="clr-namespace:Syncfusion.Maui.DataGrid;assembly=Syncfusion.Maui.DataGrid"
             xmlns:vm="clr-namespace:TripBliss.ViewModels.DistributorsViewModels.ResponseDetails"
             xmlns:Controls="clr-namespace:TripBliss.Controls"
             xmlns:ex="clr-namespace:TripBliss.Extensions"
             BackgroundColor="White"
             IsEnabled="{Binding IsBusy}"
             NavigationPage.HasNavigationBar="False"
             Title="RequestDetailsPage">
    <Controls:CustomControl.Resources>
        <Style TargetType="syncfusion:DataGridCell">
            <Setter Property="FontSize" Value="14"/>
        </Style>
    </Controls:CustomControl.Resources>
    <StackLayout Orientation="Vertical">
        <tabView:SfTabView TabBarPlacement="Bottom" VerticalOptions="FillAndExpand" IndicatorBackground="#1c874b"
                               IndicatorPlacement="Top" TabBarHeight="55" FlowDirection="LeftToRight">
            <tabView:SfTabView.Items>
                <!--Request Details-->
                <tabView:SfTabItem Header="{ex:Translate Name=btnRequestDetails-RequestDetails}" FontSize="14" BackgroundColor="#f6fcf2"
                                   FontFamily="Almarai-Regular">
                    <tabView:SfTabItem.Content>
                        <ContentView x:Name="DetailsView" Margin="{OnPlatform Android=20, iOS='20,0,20,0'}">
                            <!--<ContentView.BindingContext>
                                <vm:RequestDetailsViewModel></vm:RequestDetailsViewModel>
                            </ContentView.BindingContext>-->
                            <StackLayout Orientation="Vertical" Spacing="10">
                                <StackLayout Orientation="Horizontal">
                                    <Image VerticalOptions="CenterAndExpand">
                                        <Image.Source>
                                            <FontImageSource Glyph="ï„„" FontFamily="FontIconSolid" Color="Gray" Size="25"></FontImageSource>
                                        </Image.Source>
                                        <Image.Triggers>
                                            <DataTrigger TargetType="Image" Binding="{Binding Lang}" Value="ar">
                                                <Setter Property="Rotation" Value="180"></Setter>
                                            </DataTrigger>
                                        </Image.Triggers>
                                        <Image.GestureRecognizers>
                                            <TapGestureRecognizer Command="{Binding BackButtonCommand}"></TapGestureRecognizer>
                                        </Image.GestureRecognizers>
                                    </Image>
                                    <Label Text="{Binding Response.RequestName}" FontAutoScalingEnabled="False" FontSize="18" FontAttributes="Bold" HorizontalOptions="CenterAndExpand"
                   TextColor="Black" VerticalOptions="CenterAndExpand" ></Label>
                                </StackLayout>
                                <ScrollView VerticalOptions="FillAndExpand" VerticalScrollBarVisibility="Never">
                                    <StackLayout Spacing="{OnPlatform Android=20, iOS=0}">
                                        <!--HotelService-->
                                        <CarouselView ItemsSource="{Binding Response.ResponseWithDistributorHotel}" HorizontalOptions="FillAndExpand" HorizontalScrollBarVisibility="Never" Loop="False" WidthRequest="{OnPlatform iOS=350}" HeightRequest="{OnPlatform iOS=300}">
                                            <CarouselView.ItemTemplate>
                                                <DataTemplate>

                                                    <StackLayout Orientation="Vertical" HorizontalOptions="FillAndExpand" Margin="0,10">
                                                        <Label Text="{ex:Translate Name=lblHotelServices}" FontAutoScalingEnabled="False" TextColor="DarkOrange" Margin="0,0,0,10" FontAttributes="Bold" FontSize="16">

                                                        </Label>

                                                        <StackLayout Spacing="10" HorizontalOptions="FillAndExpand">

                                                            <Border StrokeShape="RoundRectangle 8" StrokeThickness="1" Padding="0,0,0,10"
                                                                    HorizontalOptions="FillAndExpand">
                                                                <StackLayout Orientation="Vertical" >
                                                                    <StackLayout Orientation="Horizontal">
                                                                        <Label VerticalTextAlignment="Center" Padding="5" FontAutoScalingEnabled="False">
                                                                            <Label.FormattedText>
                                                                                <FormattedString>
                                                                                    <Span Text="{Binding RequestTravelAgencyHotel.LocationName}" TextColor="Black" FontSize="14" FontAttributes="Bold"></Span>
                                                                                    <Span Text=" - " TextColor="Black" FontSize="14" FontAttributes="Bold"></Span>
                                                                                    <Span Text="{Binding RequestTravelAgencyHotel.HotelName}" TextColor="Black" FontSize="14" FontAttributes="Bold"></Span>
                                                                                </FormattedString>
                                                                            </Label.FormattedText>
                                                                        </Label>
                                                                        <Line StrokeThickness="1" Y1="0" Y2="40" Stroke="Gray" HorizontalOptions="EndAndExpand"></Line>
                                                                        <VerticalStackLayout Margin="10,0" VerticalOptions="CenterAndExpand">
                                                                            <Label Text="{Binding RequestTravelAgencyHotel.NumberOfNights}" FontAutoScalingEnabled="False" FontSize="11" TextColor="#387955" FontAttributes="Bold" HorizontalTextAlignment="Center"></Label>
                                                                            <Label Text="{ex:Translate Name=lblNights-RequestDetails}" FontAutoScalingEnabled="False" FontSize="11" TextColor="#387955"
                                                       FontAttributes="Bold"></Label>
                                                                        </VerticalStackLayout>
                                                                    </StackLayout>
                                                                    <BoxView HorizontalOptions="FillAndExpand" Color="Gray" HeightRequest=".5"></BoxView>
                                                                    <VerticalStackLayout>
                                                                        <StackLayout Orientation="Horizontal" Padding="5">
                                                                            <Label Text="{ex:Translate Name=lblCheckIn-HotelService}" FontAutoScalingEnabled="False" TextColor="Black" FontSize="13"
                                       FontFamily="Almarai-Regular"></Label>
                                                                            <Label Text=": " TextColor="Black" FontSize="13" FontAutoScalingEnabled="False"></Label>
                                                                            <Label Text="{Binding RequestTravelAgencyHotel.CheckIn , StringFormat='{0:d}'}" FontAutoScalingEnabled="False" TextColor="Gray" FontSize="12" VerticalTextAlignment="End"></Label>
                                                                            <Label Text="{ex:Translate Name=lblCheckOut-HotelService}" FontAutoScalingEnabled="False" TextColor="Black" FontSize="13"
                                       FontFamily="Almarai-Regular" HorizontalOptions="EndAndExpand"></Label>
                                                                            <Label Text=": " TextColor="Black" FontSize="13" FontAutoScalingEnabled="False"></Label>
                                                                            <Label Text="{Binding RequestTravelAgencyHotel.CheckOut ,StringFormat='{0:d}'}" FontAutoScalingEnabled="False" TextColor="Gray" FontSize="12" VerticalTextAlignment="End"></Label>
                                                                        </StackLayout>
                                                                        <Label Text="{ex:Translate Name=lblRoomDetails-RequestDetails}" FontAutoScalingEnabled="False" TextColor="#387955" FontSize="14"
                                   FontAttributes="Bold" Margin="0,5" Padding="5"></Label>
                                                                        <Border StrokeShape="RoundRectangle 8" StrokeThickness="1" Padding="7" Margin="10,0">
                                                                            <VerticalStackLayout Spacing="3">
                                                                                <StackLayout Orientation="Horizontal">

                                                                                    <Label Text="{ex:Translate Name=lblRoomType-HotelService}" FontAutoScalingEnabled="False" TextColor="Gray" FontSize="14"
                                               FontAttributes="Bold"></Label>
                                                                                    <Label Text=": " TextColor="Gray" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                    <Label Text="{Binding RequestTravelAgencyHotel.RoomTypeName}" TextColor="DarkOrange" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold"></Label>

                                                                                    <Label Text="{ex:Translate Name=lblView-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Gray" FontSize="14"
                                               FontAttributes="Bold" HorizontalOptions="EndAndExpand"></Label>
                                                                                    <Label Text=": " TextColor="Gray" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                    <Label Text="{Binding RequestTravelAgencyHotel.RoomViewName}" FontAutoScalingEnabled="False" TextColor="DarkOrange" FontSize="14" FontAttributes="Bold"></Label>
                                                                                </StackLayout>
                                                                                <StackLayout Orientation="Horizontal">
                                                                                    <Label Text="{ex:Translate Name=lblNoOfRoom-HotelService}" FontAutoScalingEnabled="False" TextColor="Gray" FontSize="14"
                                               FontAttributes="Bold"></Label>
                                                                                    <Label Text=": " TextColor="Gray" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold"></Label>
                                                                                    <Label Text="{Binding RequestTravelAgencyHotel.RoomCount}" FontAutoScalingEnabled="False" TextColor="DarkOrange" FontSize="14" FontAttributes="Bold"></Label>
                                                                                    <Label Text="{ex:Translate Name=lblMeals-HotelService}" FontAutoScalingEnabled="False" TextColor="Gray" FontSize="14"
                                               HorizontalOptions="EndAndExpand" FontAttributes="Bold"></Label>
                                                                                    <Label Text=": " TextColor="Gray" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                    <Label Text="{Binding RequestTravelAgencyHotel.MealName}" FontAutoScalingEnabled="False" TextColor="DarkOrange" FontSize="14" FontAttributes="Bold"></Label>
                                                                                </StackLayout>
                                                                                <StackLayout Orientation="Horizontal">
                                                                                    <Label Text="{ex:Translate Name=lblRatePR-RequestDetails-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Gray" FontSize="14"
                                               FontAttributes="Bold"></Label>
                                                                                    <Label Text=": " TextColor="Gray" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                    <Label Text="70" TextColor="DarkOrange" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                    <Label Text="$" TextColor="DarkOrange" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                    <Label Text="{ex:Translate Name=lblRateBasis-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Gray" FontSize="14"
                                               HorizontalOptions="EndAndExpand" FontAttributes="Bold"></Label>
                                                                                    <Label Text=": " TextColor="Gray" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                    <Label Text="2B/S" TextColor="DarkOrange" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold"></Label>
                                                                                </StackLayout>
                                                                                <StackLayout Orientation="Horizontal">
                                                                                    <Label Text="{ex:Translate Name=lblMRPD-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Gray"
                                               FontSize="14" FontAttributes="Bold"></Label>
                                                                                    <Label Text=": " TextColor="Gray" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                    <Label Text="10" TextColor="DarkOrange" FontSize="14" FontAutoScalingEnabled="False" FontAttributes="Bold"></Label>
                                                                                </StackLayout>
                                                                            </VerticalStackLayout>
                                                                        </Border>
                                                                    </VerticalStackLayout>
                                                                </StackLayout>
                                                            </Border>
                                                        </StackLayout>
                                                    </StackLayout>
                                                </DataTemplate>
                                            </CarouselView.ItemTemplate>

                                        </CarouselView>

                                        <!--TransportationServices-->
                                        <StackLayout Spacing="10">
                                            <Label Text="{ex:Translate Name=lblTransportationServices}" FontAutoScalingEnabled="False" TextColor="DarkOrange" FontAttributes="Bold" FontSize="16"
                                                   ></Label>
                                            <syncfusion:SfDataGrid ItemsSource="{Binding Response.ResponseWithDistributorTransport}" ColumnWidthMode="FitByCell"
                                           GridLinesVisibility="Both" HeaderGridLinesVisibility="Both"
                                           VerticalOptions="FillAndExpand">
                                                <syncfusion:SfDataGrid.Columns>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.Date" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblDate-TranService}" FontAutoScalingEnabled="False" TextColor="Black" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.FromLocation" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblFrom-TranService}" FontAutoScalingEnabled="False" TextColor="Black" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.ToLocation" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblTo-TranService}" FontAutoScalingEnabled="False" TextColor="Black" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.BrandName" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=hdrvehicle-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Black" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.TransportCount" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblNo-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Black" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.IsDriver" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblIsDriver-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Black" 
                                           FontAttributes="Bold" VerticalTextAlignment="Center"
                                           HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.TypeName" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblTypeName-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Black" 
                                           FontAttributes="Bold" VerticalTextAlignment="Center"
                                           HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.ModelName" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblModelName-RequestDetails}" FontAutoScalingEnabled="False" TextColor="Black" 
                                           FontAttributes="Bold" VerticalTextAlignment="Center"
                                           HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>

                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.Notes" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblNotes-HotelService}" FontAutoScalingEnabled="False" TextColor="Black" 
                                           FontAttributes="Bold" VerticalTextAlignment="Center"
                                           HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport.BrandName" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="BrandName" TextColor="Black" FontAutoScalingEnabled="False" 
                                           FontAttributes="Bold" VerticalTextAlignment="Center"
                                           HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="Id" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="Id" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>

                                                    <syncfusion:DataGridTextColumn MappingName="ResponseWithDistributorId" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="Response With Distributor Id" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>

                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransportId" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="Request Travel Agency Transport Id" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>

                                                    <syncfusion:DataGridTextColumn MappingName="Price" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="Price" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>

                                                    <syncfusion:DataGridTextColumn MappingName="Total" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="Total" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>

                                                    <syncfusion:DataGridTextColumn MappingName="Notes" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="Notes" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>

                                                    <syncfusion:DataGridTextColumn MappingName="AcceptPriceDis" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="Accept Price Dis" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>

                                                    <syncfusion:DataGridTextColumn MappingName="AcceptAgen" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="Accept Agen" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyTransport" HeaderTextAlignment="Center" Visible="False">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="RequestTravelAgencyTransport" TextColor="Black" 
                   FontAttributes="Bold" VerticalTextAlignment="Center" 
                   FontAutoScalingEnabled="False" HorizontalOptions="CenterAndExpand"/>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>

                                                </syncfusion:SfDataGrid.Columns>
                                                <syncfusion:SfDataGrid.Triggers>
                                                    <DataTrigger TargetType="syncfusion:SfDataGrid" Binding="{Binding Lang}" Value="en">
                                                        <Setter Property="FlowDirection" Value="LeftToRight"></Setter>
                                                    </DataTrigger>
                                                    <DataTrigger TargetType="syncfusion:SfDataGrid" Binding="{Binding Lang}" Value="ar">
                                                        <Setter Property="FlowDirection" Value="RightToLeft"></Setter>
                                                    </DataTrigger>
                                                </syncfusion:SfDataGrid.Triggers>
                                            </syncfusion:SfDataGrid>
                                            <StackLayout.Triggers>
                                                <DataTrigger TargetType="StackLayout" Binding="{Binding Response.ResponseWithDistributorTransport.Count}" Value="0">
                                                    <Setter Property="IsVisible" Value="False"></Setter>
                                                    <Setter Property="HeightRequest" Value="0"></Setter>
                                                </DataTrigger>
                                                <DataTrigger TargetType="StackLayout" Binding="{Binding Response.ResponseWithDistributorTransport.Count}" Value="1">
                                                    <Setter Property="HeightRequest" Value="150"></Setter>
                                                </DataTrigger>
                                                <DataTrigger TargetType="StackLayout" Binding="{Binding Response.ResponseWithDistributorTransport.Count}" Value="2">
                                                    <Setter Property="HeightRequest" Value="190"></Setter>
                                                </DataTrigger>
                                                <DataTrigger TargetType="StackLayout" Binding="{Binding Response.ResponseWithDistributorTransport.Count}" Value="3">
                                                    <Setter Property="HeightRequest" Value="240"></Setter>
                                                </DataTrigger>
                                                <DataTrigger TargetType="StackLayout" Binding="{Binding Response.ResponseWithDistributorTransport.Count}" Value="4">
                                                    <Setter Property="HeightRequest" Value="290"></Setter>
                                                </DataTrigger>
                                                <DataTrigger TargetType="StackLayout" Binding="{Binding Response.ResponseWithDistributorTransport.Count}" Value="5">
                                                    <Setter Property="HeightRequest" Value="340"></Setter>
                                                </DataTrigger>
                                                <DataTrigger TargetType="StackLayout" Binding="{Binding Response.ResponseWithDistributorTransport.Count}" Value="6">
                                                    <Setter Property="HeightRequest" Value="390"></Setter>
                                                </DataTrigger>
                                            </StackLayout.Triggers>
                                        </StackLayout>

                                        <!--AirFlightService-->
                                        <StackLayout Spacing="10" HeightRequest="250">
                                            <Label Text="{ex:Translate Name=lblAirFlightServices}" TextColor="DarkOrange" FontAutoScalingEnabled="False" FontAttributes="Bold" FontSize="16"
                           ></Label>
                                            <syncfusion:SfDataGrid ItemsSource="{Binding Response.ResponseWithDistributorAirFlight}" ColumnWidthMode="FitByCell" 
                                           GridLinesVisibility="Both" HeaderGridLinesVisibility="Both" 
                                           VerticalOptions="FillAndExpand">
                                                <syncfusion:SfDataGrid.Columns>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyAirFlight.Date" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate lblDate-TranService}" TextColor="Black" FontAutoScalingEnabled="False"
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyAirFlight.AirLine" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblCarrier-AirService}" TextColor="Black" FontAutoScalingEnabled="False" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyAirFlight.ClassName" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblClass-AirService}" TextColor="Black" FontAutoScalingEnabled="False" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyAirFlight.AirportFrom" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblFrom-TranService}" TextColor="Black" FontAutoScalingEnabled="False" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyAirFlight.AirportTo" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
                                                            <DataTemplate>
                                                                <Label FontSize="14" Text="{ex:Translate Name=lblTo-TranService}" TextColor="Black" FontAutoScalingEnabled="False" 
                                               FontAttributes="Bold" VerticalTextAlignment="Center"
                                               HorizontalOptions="CenterAndExpand"></Label>
                                                            </DataTemplate>
                                                        </syncfusion:DataGridTextColumn.HeaderTemplate>
                                                    </syncfusion:DataGridTextColumn>
                                                    <syncfusion:DataGridTextColumn MappingName="RequestTravelAgencyAirFlight.ETD" HeaderTextAlignment="Center">
                                                        <syncfusion:DataGridTextColumn.HeaderTemplate>
